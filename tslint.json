{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "ter-indent": [
            true,
            4,
            { "SwitchCase": 2 }
        ],
        "variable-name": false,
        "import-name": false,
        "no-unused-variable": true,
        "eofline": true,
        "no-trailing-whitespace": true,
        "strict-boolean-expressions": [
            true,
            "allow-undefined-union",
            "allow-null-union",
            "allow-string"
        ]
    },
    "rulesDirectory": []
}
